{
  "name": "dotfiles",
  "remoteUser": "vscode",
  "dockerComposeFile": ["docker-compose.yml"],
  "service": "vscode",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
		"foxundermoon.shell-format",
		"codezombiech.gitignore",
		"esbenp.prettier-vscode",
		"dunstontc.viml",
		"dotiful.dotfiles-syntax-highlighting"
	]
    }
  }
}
